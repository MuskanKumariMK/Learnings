<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MAUI_Test_App.INotifyProvideDemo"
             Title="INotifyProvideDemo">
    <VerticalStackLayout>
          <Entry x:Name="idtxt"
       VerticalOptions="Center"
       HorizontalOptions="Center"
       Text="{Binding Id}"/>
          <Entry x:Name="nametxt"
  VerticalOptions="Center"
  HorizontalOptions="Center"
  Text="{Binding Name}"/>
          <Entry x:Name="agetxt"
  VerticalOptions="Center"
  HorizontalOptions="Center"
  Text="{Binding Age}"/>
          <Entry x:Name="salarytxt"
VerticalOptions="Center"
HorizontalOptions="Center"
Text="{Binding Salary}"/>
          <Button Text="Change" Clicked="Button_Clicked"/>
     </VerticalStackLayout>
</ContentPage>